package eventos;

import java.awt.Point;
import java.util.ArrayList;
import java.util.List;


public class Linea {
     List<Point> points = new ArrayList<>(); // Lista para almacenar los puntos dibujados


    //GETTERS AND SETTERS
    public  List<Point> getPoints() {
        return points;
    }

    public  void setPoints(List<Point> points) {
        points = points;
    }
}
